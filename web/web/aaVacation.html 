        <script>
        	function a_click(o){
			   var as=o.parentNode.getElementsByTagName('li');
			   for(var i=0;i<as.length;i++){
				  as[i].style.backgroundColor='';
			   }
			   o.style.backgroundColor='#F5F7FA';
			}
        </script>

